@model List<Cibb.Web.BLL.Reports.Models.PreviousModel>
@{
    ViewBag.Title = "历届论坛——中国电器工业协会工业锅炉分会";
}
@section Header{
    <div id="widtop04_common"></div>
    <div id="widtop05"></div>
}
<div class="widtop08"></div>
<div id="widcenter09">
    <div id="center09_01">
        <!--kaishi-->
        <div id="jiebutton">
            @foreach (var item in Model)
            {
                <div>@item.ForumSimpleName</div>
            }
        </div>
        <!--jieshu-->
        <!--kaishi-->
        <div id="jiebutton_div">
            <!--1-->
            @foreach (var item in Model)
            {
                <div>
                    <div class="center_bjiaotinian">@item.ForumName</div>

                    <div class="center_zhuti">主题报告</div>
                    <div>
                        <ul>
                           @Html.Action("PreviousList","Report",new { forumid=item.ForumID,category=1})
                        </ul>
                    </div>
                    <div class="center_bjiaoti"></div>
                    <div class="center_zhuti">专题报告</div>
                    <div>
                        <ul>
                            @Html.Action("PreviousList", "Report", new { forumid = item.ForumID, category = 2})
                        </ul>
                    </div>
                </div>
            }
        </div>
        <!--jieshu-->
        <div class="bottom"></div>
    </div>
</div>
